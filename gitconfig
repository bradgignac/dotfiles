  signingkey = ssh-rsa 

[core]
  editor = code --wait
  excludesfile = ~/.gitignore

[credential]
  helper = osxkeychain

[color]
  ui = true

[diff]
  tool = vimdiff

[github]
  user = bradgignac

[push]
  default = matching

[url "https://"]
    insteadOf = git://

[commit]
	gpgsign = true

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

[pager]
	branch = false

[gpg]
  format = ssh

[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
